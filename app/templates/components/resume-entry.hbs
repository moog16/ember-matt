<div class="island greybox">
  <h4 class='brand'>{{entry.companyName}}</h4>
  <div>{{entry.title}}</div>
  <div>
    <span class='weight--light push-half--left'>{{date-range entry.startDate entry.endDate}}</span>
  </div>
  <div>
    <span class='weight--light push-half--left'>{{entry.locationCity}}, {{entry.locationState}}</span>
  </div>
  <p>{{entry.description}}</p>
</div>
